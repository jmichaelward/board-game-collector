{
  "name": "board-game-collector",
  "version": "0.1.0",
  "description": "Connect to the BoardGameGeek API to retrieve and adapt data for use by WordPress.",
  "main": "app/src/index.js",
  "repository": "git@github.com:jmichaelward/board-game-collector.git",
  "author": "Jeremy Ward <jeremy@jmichaelward.com>",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "start": "parcel start ./app/src/index.js ./app/src/blocks/*/*.js --out-dir=./app/dist --no-source-maps",
    "watch": "parcel watch ./app/src/index.js ./app/src/blocks/*/*.js --out-dir=./app/dist --no-source-maps",
    "build": "parcel build ./app/src/index.js ./app/src/blocks/*/*.js --out-dir=./app/dist --no-source-maps"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "@wordpress/components": "^8.4.0",
    "@wordpress/element": "^2.7.0",
    "parcel-bundler": "^1.12.3"
  },
  "dependencies": {
    "axios": "^0.19.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  }
}
